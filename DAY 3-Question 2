/*to find subarray such that the elemnts sum up to given sum*/

#include <stdio.h>
#include<stdlib.h>

void display(int arr[],int n,int i,int j)
{
    int s=i,r=j;
    int len=j-i;
 	int sub[n];		
 	for(int k=0;k<len;k++)
 	{
 		sub[k]=arr[s];
 		s++;
 	}
 	printf("THE SUBARRAY IS\n");
 	for(int z=0;z<len;z++)
 	{
 		printf("%d ",sub[z]);
 	}
 	
 	printf("\nIndices range from %d to %d",i,j-1);
}

void main()
 {
 	int n;
 	printf("Enter the number of elements of the array\n");
 	scanf("%d",&n);
 	int arr[n];
 	printf("Enter the elements of the array\n");
 	for(int i=0;i<n;i++)
 	{
 		scanf("%d",&arr[i]);
 	}
 	int sum=0;
 	printf("Enter the sum\n");
 	scanf("%d",&sum);
 	
 	int ssum;
 	for(int i=0;i<n;i++,ssum=0)
 	{
 		for(int j=i;j<n;j++)
 		{
 			if(ssum<sum)
 			{
 				ssum=ssum+arr[j];
 			}
 			else if(ssum==sum)
 			{
 			    display(arr,n,i,j);
 			    exit(0);
 			    
 			}
 			else 
 			{
 				break;
 			}
 		}
 	}
    printf("Not possible to obtain the sum");		
}
