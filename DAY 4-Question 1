#include<stdio.h>
#include<string.h>
#include<ctype.h>
void main() 
{
      char s[100];
      int i;
      printf("ENTER STRING-");
      scanf("%[^\n]%*c",s);
     
      if(isupper(s[0]));  
         printf("%c",s[0]);
      for(i=1;i<strlen(s);i++)
      {
             if(s[i]==' '&&isupper(s[i+1]))
             {
                  printf("%c",s[i+1]);
             }
       }
}
